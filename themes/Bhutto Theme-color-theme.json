{
	"name": "Bhutto Theme",
	"colors": {
		"editor.background": "#080808",
		"editor.foreground": "#ffffff",
		"editorCursor.foreground": "#78a5ff",
		"editor.selectionBackground": "#00ffea",
		"editor.lineHighlightBackground": "#202020"
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#2d8f00"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ffd000"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#00ffea"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#00ff2a"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#d2aaff"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#00ffea"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#eeff00"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#e0fffc"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#d65656"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#ffffff"
			}
		}
	]
}
